{
    "version": 2,
    "builds": [
      {
        "src": "express.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "express.js"
      }
    ],
    "env": {
      "MONGO_URI": "@mongo_uri",
      "PORT": "5000"
    },
    "ignoreCommand": "echo 'Ignoring node_modules and .env'",
    "functions": {
      "api/**/*.js": { "runtime": "nodejs18.x" }
    }
  }
  